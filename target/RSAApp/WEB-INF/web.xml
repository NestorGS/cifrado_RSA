<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="3.1">
  <servlet>
    <servlet-name>EncryptServlet</servlet-name>
    <servlet-class>com.nestorgs.EncryptServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EncryptServlet</servlet-name>
    <url-pattern>/EncryptServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DecryptServlet</servlet-name>
    <servlet-class>com.nestorgs.DecryptServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DecryptServlet</servlet-name>
    <url-pattern>/DecryptServlet</url-pattern>
  </servlet-mapping>
</web-app>
